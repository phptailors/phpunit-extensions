{
    "name": "phptailors/phpunit-common",
    "description": "Common objects used by other phptailors/phunit-* packages",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
      "issues": "https://github.com/phptailors/phpunit-extensions/issues",
      "source": "https://github.com/phptailors/phpunit-extensions"
    },
    "authors": [
        {
            "name": "Pawe≈Ç Tomulik",
            "email": "pawel@tomulik.pl"
        }
    ],
    "conflict": {
        "nikic/php-parser": "< 4.13.2"
    },
    "require": {
        "php": ">=7.3",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.5 || ^10",
        "sebastian/exporter": "^4 || ^5",
        "sebastian/recursion-context": "^4 || ^5"
    },
    "autoload": {
      "psr-4" : {
        "Tailors\\PHPUnit\\" : "src/"
      },
      "files": [
      ]
    },
    "autoload-dev": {
      "psr-4" : {
        "Tailors\\PHPUnit\\" : "tests/src/"
      }
    }
}
