{
    "name": "phptailors/phpunit-common",
    "description": "Common objects used by other phptailors/phunit-* packages",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
      "issues": "https://github.com/phptailors/phpunit-extensions/issues",
      "source": "https://github.com/phptailors/phpunit-extensions"
    },
    "authors": [
        {
            "name": "Pawe≈Ç Tomulik",
            "email": "ptomulik@meil.pw.edu.pl"
        }
    ],
    "conflict": {
        "nikic/php-parser": "< 4.13.2"
    },
    "require": {
        "php": ">=7.4",
        "ext-mbstring": "*",
        "composer/semver": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10 || ^10"
    },
    "autoload": {
      "psr-4" : {
        "Tailors\\PHPUnit\\" : "src/"
      },
      "files": [
          "autoload.php"
      ]
    },
    "autoload-dev": {
      "psr-4" : {
        "Tailors\\PHPUnit\\" : "tests/src/"
      }
    }
}
